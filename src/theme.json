{
  "simpleContainer": {
    "display": "flex",
    "align-items": "center",
    "align-content": "stretch",
    "borderWidth": "1px",
    "borderStyle": "solid",
    "borderColor": "#ffffff",
    "fontFamily": "verdana, arial, helvetica, sans-serif",
    "fontSize": "x-small",
    "padding": 0,
    "width": "max-content"
  },

  "label": {
    "cursor": "pointer",
    "width": "85%",
    "width": "-moz-available",
    "width": "-webkit-fill-available",
    "width": "fill-available",
    "borderWidth": "1px",
    "borderStyle": "solid",
    "borderColor": "#ffffff",
    "padding": 0
  },

  "content": {
    "borderWidth": "1px",
    "borderStyle": "solid",
    "borderColor": "#ffffff",
    "padding": 0
  },
  "Boolean": {
    "container": {
      "borderColor": "#eebb00"
    },
    "label": {
      "borderColor": "#eebb00",
      "backgroundColor": "#ffda75",
      "color": "#222222",
      "padding": 3
    },
    "content": {
      "borderColor": "#eebb00",
      "backgroundColor": "#ffffff",
      "color": "#222222",
      "padding": 3
    }

  },


  "pre": {
    "textAlign": "left",
    "lineHeight": "normal",
    "backgroundColor": "#ffffff",
    "border": 0,
    "padding": 0
  },
  "code": {
    "fontSize": "small",
    "fontFamily": "Consolas, Menlo, Monaco, Lucida Console, monospace, Courier New, monospace, serif"
  },


  "object": {
    "backgroundColor": "#0000cc",
    "borderColor": "#0000cc"
  },

  "number": {
    "borderColor": "#ff8833",
    "data": {
      "backgroundColor": "#ffffff",
      "color": "#222222"
    },
    "label": {
      "backgroundColor": "#ffb885",
      "color": "#222222"
    }
  },

  "String": {
    "container": {
      "borderColor": "#888888"
    },
    "label": {
      "borderColor": "#888888",
      "backgroundColor": "#dddddd",
      "color": "#222222",
      "padding": 3
    },
    "content": {
      "borderColor": "#888888",
      "backgroundColor": "#ffffff",
      "color": "#222222",
      "padding": 3
    }
  }
}
